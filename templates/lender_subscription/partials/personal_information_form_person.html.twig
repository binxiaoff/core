                        {% trans with {'%tosStart%' : '<a href="' ~ termsOfUsePerson ~ '" target="_blank">', '%tosEnd%' : '</a>'} %} lender-subscription_terms-of-use-acceptance {% endtrans %}
                        <span class="field-required">*<span class="sr-only">{% trans %} common_mandatory-field {% endtrans %}
                            </span>
                        </span>
                    </span>
                </label>
            </div>
        </div>

        <div class="row">
            <div class="form-field form-field-single-checkbox{% if identityForm.noUsPerson.vars.errors|length > 0 %} ui-formvalidation-error{% endif %}">
                <label>
                    {{ form_widget(identityForm.noUsPerson) }}
                    <span class="label">{% trans %} common_no-us-person-declaration {% endtrans %}</span>
                </label>
            </div>
        </div>

        <div class="row">{% trans %} lender-subscription_newsletter-consent-description {% endtrans %}</div>

        <div class="row">
            <div class="form-field form-field-single-checkbox">
                <label>
                    {{ form_widget(identityForm.client.optin1) }}
                    <span class="label">{% trans %} lender-subscription_newsletter-consent-label {% endtrans %}</span>
                </label>
            </div>
        </div>
    </fieldset>

    {% if app.session.get(constant('Unilend\\Controller\\LenderSubscriptionController::SESSION_NAME_CAPTCHA')) %}
        {% import 'partials/form_elements.html.twig' as forms %}
        <fieldset class="panel">
            <div class="row">
                {# As long as personal_information_form_legal_entity.html.twig is included after, we only peek data here in order not to reset it before displaying other form #}
                {% for message in app.session.flashBag.peek('lenderSubscriptionCaptchaError') %}
                    <div class="message-error">
                        <span>{{ message }}</span>
                    </div>
                {% endfor %}
                {{ forms.recaptcha(recaptchaKey) }}
            </div>
        </fieldset>
    {% endif %}

    <div class="row btn-controls tab-footer">
        <div class="col-back col-lg-6 col-md-6">&nbsp;</div>
        <div class="col-next col-lg-6 col-md-6">
            <button type="submit" class="btn-primary" data-spinnerbutton>
                {% trans %} lender-subscription_part1-validate-button {% endtrans %}
                <span class="icon fa-angle-right"></span>
            </button>
        </div>
    </div><!-- /.btn-controls.tab-footer -->
{{ form_end(identityForm) }}
