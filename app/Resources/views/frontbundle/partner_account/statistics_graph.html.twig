{% set submittedChart = {
    type: 'partnerProjects',
    highcharts: {
        title: {
            text: 'Dossiers envoyés',
        },
        xAxis: {
            categories: timeAxis,
            crosshair: true
        },
        yAxis: {
            min: 0,
            title: {
                text: ''
            },
            gridLineColor: '#efefef'
        },
        plotOptions: {
            column: {
                stacking: 'normal'
            }
        },
        legend: {
            enabled: false
        },
        series: [
            {
                name: 'Total',
                color: '#1A87DA',
                data: submittedProjects
            }
        ]
    }
} %}

