<section id="data-update-legal-entity-identity-view" role="tabpanel" class="lender-data-update-view collapse in">
    <dl class="list-2col">
        <dt>{% trans %} lender-profile_information-tab-identity-section-company-name-label {% endtrans %}</dt>
        <dd id="data-update-legal-entity-identity-company-name-output">{{ company.name }}</dd>
        <dt>{% trans %} lender-profile_information-tab-identity-section-company-legal-form-label {% endtrans %}</dt>
        <dd id="data-update-legal-entity-identity-company-form-output">{{ company.forme }}</dd>
        <dt>{% trans %} lender-profile_information-tab-identity-section-company-social-capital-label {% endtrans %}</dt>
        <dd id="data-update-legal-entity-identity-company-capital-output">{{ company.capital }}</dd>
        <dt>{% trans %} lender-profile_information-tab-identity-section-company-siren-label {% endtrans %}</dt>
        <dd>{{ company.siren }}</dd>
        <dt>{% trans %} lender-profile_information-tab-identity-section-company-phone-label {% endtrans %}</dt>
        <dd id="data-update-legal-entity-identity-company-phone-output">{{ company.phone }}</dd>
    </dl>

    <div class="btn-controls">
        <button data-target="#data-update-legal-entity-identity-edit" role="button" data-toggle="collapse" data-parent="#data-update-legal-entity-identity-section" class="btn-default"
                aria-controls="#data-update-legal-entity-identity-edit" aria-expanded="false" type="button">
            <span class="icon fa-pencil-u32"></span>
            <span class="label">{% trans %} lender-data-update_modify-button {% endtrans %}</span>
        </button>
        <button data-target="#panel-data-update-main-address" role="button" data-toggle="collapse" data-parent="#data-update-section" class="btn-primary data-update-continue-btn"
                aria-controls="#panel-data-update-main-address" aria-expanded="false" type="button">
            <span class="label">{% trans %} lender-data-update_continue-button {% endtrans %}</span>
        </button>
    </div>
</section>

<section id="data-update-legal-entity-identity-edit" role="tabpanel" class="lender-data-update-edit collapse" data-formvalidation data-formvalidation-notificationselem="#data-update-legal-entity-identity-edit-notifications">
    <div id="data-update-legal-entity-identity-edit-notifications"></div>
    <div class="row row-multi-col-computer">
        <div class="form-field col-lg-6 col-md-6 {% if form.company.name.vars.errors|length > 0 %}ui-formvalidation-error{% endif %}">
            <label for="form-data-update-legal-entity-identity-company-name">{% trans %} lender-profile_information-tab-identity-section-company-name-label {% endtrans %}</label>
            {{ form_widget(form.company.name, {
                'id': 'form-data-update-legal-entity-identity-company-name',
                'required': false,
                'attr': {
                    'class': 'input-field',
                    'data-formvalidation-input data-formvalidation-required': 'true',
                    'data-impacted-element-id': 'data-update-legal-entity-identity-company-name-output',
                    'data-original-value': company.name
                }
            }) }}
            {{ form_errors(form.company.name) }}
        </div><!-- /.form-field -->

        <div class="form-field col-lg-6 col-md-6 {% if form.company.forme.vars.errors|length > 0 %}ui-formvalidation-error{% endif %}">
            <label for="form-data-update-legal-entity-identity-company-legal-form">{% trans %} lender-profile_information-tab-identity-section-company-legal-form-label {% endtrans %}
                <span class="field-required">*<span class="sr-only"> {% trans %} common_mandatory-field {% endtrans %}</span></span></label>
            {{ form_widget(form.company.forme, {
                'id': 'form-data-update-legal-entity-identity-company-legal-form',
                'required': false,
                'attr': {
                    'class': 'input-field',
                    'data-formvalidation-input data-formvalidation-required': 'true',
                    'data-impacted-element-id': 'data-update-legal-entity-identity-company-form-output',
                    'data-original-value': company.forme
                }
            }) }}
            {{ form_errors(form.company.forme) }}
        </div><!-- /.form-field -->
    </div><!-- /.row -->

    <div class="row row-multi-col-computer">
        <div class="form-field col-lg-6 col-md-6 {% if form.company.capital.vars.errors|length > 0 %}ui-formvalidation-error{% endif %}">
            <label for="form-data-update-legal-entity-identity-company-social-capital">{% trans %} lender-profile_information-tab-identity-section-company-social-capital-label {% endtrans %}</label>
            {{ form_widget(form.company.capital, {
                'id': 'form-data-update-legal-entity-identity-company-social-capital',
                'required': false,
                'attr': {
                    'class': 'input-field',
                    'data-formvalidation-input data-formvalidation-required': 'true',
                    'data-impacted-element-id': 'data-update-legal-entity-identity-company-capital-output',
                    'data-original-value': company.capital
                }
            }) }}
            {{ form_errors(form.company.capital) }}
        </div><!-- /.form-field -->
        <div class="form-field col-lg-6 col-md-6">
            <label for="form-data-update-legal-entity-identity-company-siren">{% trans %} lender-profile_information-tab-identity-section-company-siren-label {% endtrans %}
                <span class="field-required">*<span class="sr-only"> {% trans %} common_mandatory-field {% endtrans %}</span></span></label>
            <span id="form-data-update-legal-entity-identity-company-siren" >{{ company.siren }}</span>
        </div><!-- /.form-field -->
    </div><!-- /.row -->

    <div class="row row-multi-col-computer">
        <div class="form-field col-lg-6 col-md-6 {% if form.company.phone.vars.errors|length > 0 %}ui-formvalidation-error{% endif %}">
            <label for="form-data-update-legal-entity-identity-company-phone">{% trans %} lender-profile_information-tab-identity-section-company-phone-label {% endtrans %}</label>
            {{ form_widget(form.company.phone, {
                'id': 'form-data-update-legal-entity-identity-company-phone',
                'required': false,
                'attr': {
                    'class': 'input-field',
                    'data-formvalidation-input data-formvalidation-required': 'true',
                    'data-impacted-element-id': 'data-update-legal-entity-identity-company-phone-output',
                    'data-original-value': company.phone
                }
            }) }}
            {{ form_errors(form.company.phone) }}
        </div><!-- /.form-field -->

        <div class="form-field col-lg-6 col-md-6"></div><!-- /.form-field -->
    </div><!-- /.row -->

    <div class="btn-controls">
        <button class="btn-primary lender-data-update-save-btn" id="lender-data-update-legal-entity-identity-save-btn" role="button" data-parent="#data-update-legal-entity-identity-section"
                aria-controls="#data-update-legal-entity-identity-view" aria-expanded="false" type="button">{% trans %} lender-data-update_save-button {% endtrans %}</button>
        <button class="btn-default" role="button" data-toggle="collapse" data-target="#data-update-legal-entity-identity-view" data-parent="#data-update-legal-entity-identity-section"
                aria-controls="#data-update-legal-entity-identity-view" aria-expanded="false" type="reset">{% trans %} lender-data-update_cancel-button {% endtrans %}</button>
    </div>
</section><!-- /.data-update-legal-entity-identity-edit -->
