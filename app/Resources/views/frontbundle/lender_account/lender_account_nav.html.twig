<nav class="dashboard-nav">
    {% block dashboardNav %}
        <ul>
            <li role="presentation" class="dashboard-nav-dashboard {% if route == 'lender_dashboard' %}active{% else %}complete{% endif %}">
                <a href="{{ path('lender_dashboard') }}">
                    <span class="icon fa-dashboard-u"></span>
                    <span class="label">{% trans %} lender-dashboard-navigation_dashboard-link {% endtrans %}</span>
                </a>
            </li>
            {% if app.user.clientStatus in constant('\\Unilend\\Bundle\\CoreBusinessBundle\\Entity\\ClientsStatus::GRANTED_LENDER_DEPOSIT') and app.user.clientStatus in constant('\\Unilend\\Bundle\\CoreBusinessBundle\\Entity\\ClientsStatus::GRANTED_LENDER_WITHDRAW') %}
                <li role="presentation" class="dashboard-nav-balance {% if route in ['lender_wallet_deposit', 'lender_wallet_deposit_result', 'lender_wallet_deposit_result_error', 'lender_wallet_withdrawal'] %}active{% else %}complete{% endif %}">
                    <a href="{{ path('lender_wallet_deposit') }}">
                        <span class="icon fa-money-u32"></span>
                        <span class="label">{% trans %} lender-dashboard-navigation_deposit-withdraw-link {% endtrans %}</span>
                    </a>
                </li>
            {% elseif app.user.clientStatus in constant('\\Unilend\\Bundle\\CoreBusinessBundle\\Entity\\ClientsStatus::GRANTED_LENDER_DEPOSIT') %}
                <li role="presentation" class="dashboard-nav-balance {% if route in ['lender_wallet_deposit', 'lender_wallet_deposit_result', 'lender_wallet_deposit_result_error', 'lender_wallet_withdrawal'] %}active{% else %}complete{% endif %}">
                    <a href="{{ path('lender_wallet_deposit') }}">
                        <span class="icon fa-money-u32"></span>
                        <span class="label">{% trans %} lender-dashboard-navigation_deposit-link {% endtrans %}</span>
                    </a>
                </li>
            {% elseif app.user.clientStatus in constant('\\Unilend\\Bundle\\CoreBusinessBundle\\Entity\\ClientsStatus::GRANTED_LENDER_WITHDRAW') %}
                <li role="presentation" class="dashboard-nav-balance {% if route in ['lender_wallet_deposit', 'lender_wallet_deposit_result', 'lender_wallet_deposit_result_error', 'lender_wallet_withdrawal'] %}active{% else %}complete{% endif %}">
                    <a href="{{ path('lender_wallet_withdrawal') }}">
                        <span class="icon fa-money-u32"></span>
                        <span class="label">{% trans %} lender-dashboard-navigation_withdraw-link {% endtrans %}</span>
                    </a>
                </li>
            {% endif %}
            <li role="presentation" class="dashboard-nav-projects {% if route == 'lender_projects' %}active{% else %}complete{% endif %}">
                <a href="{{ path('lender_projects') }}">
                    <span class="icon fa-project-u"></span>
                    <span class="label">{% trans %} lender-dashboard-navigation_projects-link {% endtrans %}</span>
                </a>
            </li>
            <li role="presentation" class="dashboard-nav-operations {% if route == 'lender_operations' %}active{% else %}complete{% endif %}">
                <a href="{{ path('lender_operations') }}">
                    <span class="icon fa-operations-u"></span>
                    <span class="label">{% trans %} lender-dashboard-navigation_operations-link {% endtrans %}</span>
                </a>
            </li>
            <li role="presentation" class="dashboard-nav-profile {% if route|slice(0, 14) == 'lender_profile' or route in ['cip_index', 'cip_start_questionnaire', 'cip_continue_questionnaire'] %}active{% else %}complete{% endif %}">
                <a href="{{ path('lender_profile') }}">
                    <span class="icon fa-profile-u"></span>
                    <span class="label">{% trans %} lender-dashboard-navigation_profile-link {% endtrans %}</span>
                </a>
            </li>
            {% if isAutobidQualified %}
                <li role="presentation" class="dashboard-nav-autolend {% if route == 'autolend' %}active{% else %}complete{% endif %}">
                    <a href="{{ path('autolend') }}">
                        <span class="icon fa-autolend-u"></span>
                        <span class="label">{% trans %} lender-dashboard-navigation_autolend-link {% endtrans %}</span>
                    </a>
                </li>
            {%  endif %}
            {% if currentSponsorshipCampaign is defined and currentSponsorshipCampaign is not empty and false == isBlacklisted %}
                <li role="presentation" class="dashboard-nav-sponsorship {% if route == 'lender_sponsorship' %}active{% else %}complete{% endif %}">
                    <a href="{{ path('lender_sponsorship') }}">
                        <span class="icon fa-network-u"></span>
                        <span class="label">{% trans %} lender-dashboard-navigation_sponsorship-link {% endtrans %}</span>
                    </a>
                </li>
            {% endif %}
        </ul>
    {% endblock %}
</nav>
