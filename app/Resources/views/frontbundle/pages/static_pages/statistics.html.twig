<section id="section-platform" class="page-section bg-bg3">
    <div class="site-wrap">
        <div class="block-stats block-stats-unilend">
            <header class="block-stats-header">
                <h2>{% trans %} about-us-statistics_unilend-performances-title {% endtrans %}</h2>
            </header>
            <div class="row">
                <h3 class="c-t1">{% trans %} about-us-statistics_unilend-performances-chart-title {% endtrans %}</h3>
                <div class="stats-chart stats-chart-unilend">
                    <div class="chart" data-chartview='{{ overviewStatistics|json_encode|escape('html_attr') }}'></div>
                    <div class="stats-chart-unilend-key">
                        <div class="row">
                            {% for i, series in overviewStatistics.highcharts.series %}
                                {% for j, dataItem in series.data if dataItem.showInLegend is defined and dataItem.showInLegend != false %}
                                    <div class="chart-key col-lg-4 col-md-4 col-sm-4 col-xs-2">
                                        <div class="icon" style="background-color: {{ dataItem.color|default(series.color) }}"></div>
                                        <div class="label">{{ dataItem.name|default(series.name) }}</div>
                                        <div class="value">{{ dataItem.y|default(series.value) }}€</div>
                                    </div>
                                {% endfor %}
                            {% endfor %}
                        </div>
                    </div><!-- /.stats-chart-unilend-key -->
                </div><!-- /.stats-chart-unilend -->
            </div>
            <div class="row">
                <h3 class="c-t1">{% trans %} about-us-statistics_unilend-performances-gross-data-title {% endtrans %}</h3>
                <div class="table-scroll">
                    <table class="table table-stats-unilend">
                        <thead>
                            <tr>
                                <th class="table-stats-unilend-item-label">&nbsp;</th>
                                <th class="table-stats-unilend-item-value">2013–2014</th>
                                <th class="table-stats-unilend-item-value">2015</th>
                                <th class="table-stats-unilend-item-value">2016</th>
                                <th class="table-stats-unilend-item-value">Total</th>
                            </tr>
                        </thead>
                        <tbody>
                            {% for i, item in stats.gross %}
                                <tr>
                                    {% if item.data is iterable %}
                                        <td class="table-stats-unilend-item-label">{{ item.label|raw }}</td>
                                        {% for j, dataItem in item.data %}
                                            <td class="table-stats-unilend-item-value">{{ dataItem|localizednumber|nbsp|raw }}</td>
                                        {% endfor %}
                                    {% else %}
                                        <td colspan="4" class="table-stats-unilend-item-label">{{ item.label|raw }}</td>
                                        <td class="table-stats-unilend-item-value">{{ item.data|raw }}</td>
                                    {% endif %}
                                </tr>
                            {% endfor %}
                        </tbody>
                    </table>
                </div><!-- /.table-scroll -->
            </div>
        </div><!-- /.block-stats -->

        <div class="btn-toolbar">
            <a href="{{ path('projects_list') }}" class="btn-primary">{% trans %} about-us-statistics_link-to-project-list {% endtrans %}</a>
        </div><!-- /.btn-toolbar -->
    </div><!-- /.site-wrap -->
</section><!-- /#section-platform -->
