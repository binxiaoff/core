{% set unilendPerformanceChart = {
    type: 'preterProjectsOverview',
    background: 'none',
    highcharts: {
        chart: {
            type: 'pie',
            plotBackgroundColor: null,
            plotBorderWidth: null,
            plotShadow: null
            },
        title: {
            text: ''
            },
        subtitle: {
            text: '',
            color: '#999999',
            floating: true,
            useHTML: true,
            verticalAlign: 'middle'
            },
        plotOptions: {
            pie: {
            borderColor: 'none',
            borderWidth: 0,
            showInLegend: true,
            dataLabels: {
                enabled: false
                }
            }
        },
        legend: {
            symbolRadius: 10
            },
        series: [{
            name: 'about-us-statistics_regulatory-table-borrowed-capital'|trans ~ ' ',
            minSize: 120,
            innerSize: '95%',
            endAngle: 270,
            data: [{
                name: 'about-us-statistics_regulatory-table-borrowed-capital'|trans,
                color: '#5FC5D1',
                y: data.regulatoryTable['borrowed-capital']['total'],
                value: data.regulatoryTable['borrowed-capital']['total']|localizedcurrency('EUR'),
                showInLegend: true,
                }]
            },{
                name:  'lender-dashboard-account_capital-circle-title'|trans ~ ' ',
                size: '66%',
                minSize: 100,
                innerSize: '88%',
                endAngle: 270,
                data: [{
                    name: 'about-us-statistics_regulatory-table-repaid-capital'|trans,
                    color: '#1B88DB',
                    y: data.regulatoryTable['repaid-capital']['total'],
                    value: data.regulatoryTable['repaid-capital']['total']|localizedcurrency('EUR'),
                    showInLegend: true,
                },{
                    name: 'about-us-statistics_chart-lost-capital'|trans,
                    color: '#B6D4E4',
                    y: data.regulatoryTable['future-owed-capital-healthy']['total'],
                    value: data.regulatoryTable['future-owed-capital-healthy']['total']|localizedcurrency('EUR'),
                    showInLegend: true
                },{
                    name: 'about-us-statistics_regulatory-table-total-owed-problematic-capital-late'|trans,
                    color: '#F76965',
                    y:  data.regulatoryTable['total-owed-problematic-capital-late']['total'],
                    value: data.regulatoryTable['total-owed-problematic-capital-late']['total']|localizedcurrency('EUR'),
                    showInLegend: true
                }]
            },{
                name: 'lender-dashboard-account_interests-circle-title'|trans ~ ' ',
                minSize: 80,
                size: '33%',
                innerSize: '70%',
                endAngle: (270 * (data.regulatoryTable['borrowed-capital']['total'] + data.regulatoryTable['repaid-interest']['total'] + data.regulatoryTable['owed-healthy-interest']['total'])) / data.regulatoryTable['borrowed-capital']['total'],
                data: [{
                    name: '',
                    color: '#ffffff',
                    dataLabels: {
                        enabled: false
                        },
                    showInLegend: false,
                    y: (data.regulatoryTable['repaid-capital']['total'] + data.regulatoryTable['future-owed-capital-healthy']['total']) * 0.66,
                    tooltip: true
                },{
                    name: 'about-us-statistics_regulatory-table-repaid-interest'|trans,
                    color: '#F2980C',
                    y: data.regulatoryTable['repaid-interest']['total'],
                    value: data.regulatoryTable['repaid-interest']['total']|localizedcurrency('EUR'),
                    showInLegend: true
                },{
                    name: 'about-us-statistics_regulatory-table-owed-healthy-interest'|trans,
                    color: '#FFCA2C',
                    y: data.regulatoryTable['owed-healthy-interest']['total'],
                    value: data.regulatoryTable['owed-healthy-interest']['total']|localizedcurrency('EUR'),
                    showInLegend: true
                }]
            }],
        tooltip: {
            valueSuffix: ' €'
            }
        }
    }
%}

