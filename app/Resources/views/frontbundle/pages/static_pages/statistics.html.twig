<section id="section-intro" class="page-section">
    <div class="site-wrap">
        <div class="row">
            <div class="col-lg-6 col-md-6">
                <header class="block-header">
                    <h1 class="h4">{% trans %} about-us-statistics_general-description-title {% endtrans %}</h1>
                </header>
            </div>
            <div class="col-lg-6 col-md-6">
                <div class="block-text">
                    {% trans with {'%numberProjectRequests%' : getStatistic('numberOfProjectRequests')|localizednumber, '%percentageAcceptedRequests%' : getStatistic('percentageOfAcceptedProjects')|localizednumber }%} about-us-statistics_general-description-text {% endtrans %}
                    <p>
                        <a href="{% trans %} about-us-statistics_link-under-general-description-target-url {% endtrans %}" class="link">
                            {% trans %} about-us-statistics_link-under-general-description-text {% endtrans %}
                        </a>
                    </p>
                </div>
            </div>
        </div>
    </div><!-- /.site-wrap -->
</section><!-- /.page-section -->

<div class="bg-bg3">
    <section id="section-preteurs" class="page-section">
        <div class="site-wrap">
            <div class="block-stats block-stats-p1">
                <header class="block-stats-header">
                    <h2>{% trans %} about-us-statistics_lender-statistics-title {% endtrans %}</h2>
                </header>
                <div class="block-promos row readable-wrap bg-icon-linechart">
                    <div class="promo col-lg-6 col-md-6 col-sm-4">
                        <h3 class="promo-highlight promo-highlight-lg">
                            {% set unilendIRR = getStatistic('unilendIRR')|round(1, 'floor') %}
                            <span data-textcount data-start-count="0" data-end-count="{{ unilendIRR }}" data-total-time="3000" data-limit-decimal="1" data-tween-count="easeOutCirc">{{ unilendIRR|localizednumber }}</span><sup>%</sup>
                        </h3>
                        <p>{% trans %} about-us-statistics_lender-statistics-linechart-unilend-irr-text {% endtrans %}</p>
                    </div><!-- /.promo -->
                    <div class="promo col-lg-6 col-md-6 col-sm-4">
                        <h3 class="promo-highlight promo-highlight-lg">
                            {% set averageInterestRateForLenders = getStatistic('averageInterestRateForLenders')|round(1, 'floor') %}
                            <span data-textcount data-start-count="0" data-end-count="{{ averageInterestRateForLenders }}" data-total-time="3000" data-limit-decimal="1" data-tween-count="easeOutCirc">{{ averageInterestRateForLenders|localizednumber }}</span><sup>%</sup>
                        </h3>
                        <p>{% trans %} about-us-statistics_lender-statistics-linechart-avg-interest-rate-text {% endtrans %}</p>
                    </div><!-- /.promo -->
                </div><!-- /.promos -->
                <div class="stats-charts row">
                    <figure class="stats-chart stats-map">
                        <div class="chart" data-chartview='{{ preterStatisticsMap|json_encode|escape('html_attr') }}'>
                            {# @todo Chart #}
                            {# <img src="{{ siteurlmedia('apropos/statistiques/map-fr-repartition.png') }}" alt="" /> #}
                        </div>
                        <figcaption><p>{% trans %} about-us-statistics_lender-statistics-map-title {% endtrans %}</p></figcaption>
                    </figure>
                    {# TODO margin-top: -175px;#}
                    <div class="stats-members">
                        <div>
                            {% set numberOfLendersInCommunity = getStatistic('numberOfLendersInCommunity') %}
                            <h3 class="promo-highlight promo-highlight-lg">
                                <span data-textcount data-start-count="0"
                                      data-end-count="{{ numberOfLendersInCommunity }}" data-total-time="3000"
                                      data-tween-count="easeOutCirc">{{ numberOfLendersInCommunity|localizednumber }}</span>
                            </h3>
                            {% trans %} about-us-statistics_lender-statistics-members-active-not-text {% endtrans %}
                        </div>
                        <div>
                            {% set numberOfActiveLenders = getStatistic('numberOfActiveLenders') %}
                            <h3 class="promo-highlight promo-highlight-lg">
                                <span data-textcount data-start-count="0" data-end-count="{{ numberOfActiveLenders }}" data-total-time="3000" data-tween-count="easeOutCirc">{{ numberOfActiveLenders|localizednumber }}</span>
                            </h3>
                            {% trans %} about-us-statistics_lender-statistics-members-type-text {% endtrans %}
                        </div>
                        <img src="{{ siteurlmedia('apropos/statistiques/icons-members.png') }}" alt="">
                    </div>
                    {% set lendersByType = getStatistic('lendersByType') %}
                    <div class="stats-enterprises">
                        <img src="{{ siteurlmedia('apropos/statistiques/icons-enterprises.png') }}" alt="">
                        <div>
                            <span class="stats-number"><span data-textcount data-start-count="0" data-end-count="{{ lendersByType.person.percentage }}" data-total-time="3000" data-tween-count="easeOutCirc">{{ lendersByType.person.percentage|localizednumber }}</span><sup>%</sup></span> {% trans %} about-us-statistics_lender-statistics-members-type-person-text {% endtrans %}
                        </div>
                        <div>
                            <span class="stats-number">{{ lendersByType.legalEntity.percentage|localizednumber }}<sup>%</sup></span>
                            {% trans %} about-us-statistics_lender-statistics-members-type-legal-entity-text {% endtrans %}</div>
                    </div>
                </div><!-- /.stats-map -->
                <div class="block-promos row">
                    <div class="promo col-lg-4 col-md-4">
                        <h3 class="promo-highlight promo-highlight-lg">
                            {% set averageNumberOfLenders = getStatistic('averageNumberOfLenders')|round %}
                            <span data-textcount data-start-count="0" data-end-count="{{ averageNumberOfLenders }}" data-total-time="3000" data-tween-count="easeOutCirc">{{ averageNumberOfLenders|localizednumber }}</span>
                        </h3>
                        <p>{% trans %} about-us-statistics_lender-statistics-1-text {% endtrans %}</p>
                    </div><!-- /.promo -->
                    <div class="promo col-lg-4 col-md-4">
                        <h3 class="promo-highlight promo-highlight-lg">
                            {% set averageLoanAmount = getStatistic('averageLoanAmount')|round|localizednumber %}
                            <span data-textcount data-start-count="0" data-end-count="{{ averageLoanAmount }}" data-total-time="3000" data-tween-count="easeOutCirc">{{ averageLoanAmount|localizednumber }}</span><sup>€</sup>
                        </h3>
                        <p>{% trans %} about-us-statistics_lender-statistics-2-text {% endtrans %}</p>
                    </div><!-- /.promo -->
                    <div class="promo col-lg-4 col-md-4">
                        <h3 class="promo-highlight promo-highlight-lg">
                            <span data-textcount data-start-count="0" data-end-count="100" data-total-time="3000" data-tween-count="easeOutCirc">100</span><sup>%</sup>
                        </h3>
                        <p>{% trans %} about-us-statistics_lender-statistics-3-text {% endtrans %}</p>
                    </div><!-- /.promo -->
                </div><!-- /.promos -->
            </div><!-- /.block-stats -->
        </div>
    </section><!-- /#section-preteurs -->

