                            {% set percentageSuccessfullyFinancedProjects = getStatistic('percentageSuccessfullyFinancedProjects')|round %}
                            <span data-textcount data-start-count="0" data-end-count="{{ percentageSuccessfullyFinancedProjects }}" data-total-time="3000" data-tween-count="easeOutCirc">{{ percentageSuccessfullyFinancedProjects }}</span><sup>%</sup>
                        </h3>
                        <p>{% trans %} about-us-statistics_borrower-statistics-3-text {% endtrans %}</p>
                    </div><!-- /.promo -->
                    <div class="promo col-lg-3 col-md-3">
                        <h3 class="promo-highlight promo-highlight-md">
                            {% set numberOfFinancedProjects = getStatistic('numberOfFinancedProjects') %}
                            <span data-textcount data-start-count="0" data-end-count="{{ numberOfFinancedProjects }}" data-total-time="3000" data-tween-count="easeOutCirc">
                                {{ numberOfFinancedProjects|localizednumber }}</span><sup></sup>
                        </h3>
                        <p>{% trans %} about-us-statistics_borrower-statistics-4-text {% endtrans %}</p>
                    </div><!-- /.promo -->
                </div><!-- /.promos -->
            </div><!-- /.block-stats -->
        </div><!-- /.site-wrap -->
    </section><!-- /#section-emprunters -->
</div><!-- /.bg-bg3 -->

