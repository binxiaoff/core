<section id="section-platform" class="page-section bg-bg3">
    <div class="site-wrap">
        <div class="block-stats block-stats-unilend">
            <header class="block-stats-header">
                <h2>{% trans %} about-us-statistics_unilend-performances-title {% endtrans %}</h2>
            </header>
            <div class="row">
                <h3 class="c-t1">{% trans %} about-us-statistics_unilend-performances-chart-title {% endtrans %}</h3>
                <div class="stats-chart stats-chart-unilend">
                    <div class="chart" data-chartview='{{ unilendPerformanceChart|json_encode|escape('html_attr') }}'></div>
                    <div class="stats-chart-unilend-key">
                        <div class="row">
                            {% for i, series in unilendPerformanceChart.highcharts.series %}
                                {% for j, dataItem in series.data if dataItem.showInLegend is defined and dataItem.showInLegend != false %}
                                    <div class="chart-key col-lg-4 col-md-4 col-sm-4 col-xs-2">
                                        <div class="icon" style="background-color: {{ dataItem.color }}"></div>
                                        <div class="label">{% if dataItem.name is not empty %} {{ dataItem.name }} {% else %} {{ series.name }} {% endif %}</div>
                                        <div class="value">{% if dataItem.value is not empty %} {{ dataItem.value }} {% else %} {{ series.value }} {% endif %}</div>
                                    </div>
                                {% endfor %}
                            {% endfor %}
                        </div>
                    </div><!-- /.stats-chart-unilend-key -->
                </div><!-- /.stats-chart-unilend -->
            </div>
            <div class="row">
                <h3 class="c-t1">{% trans %} about-us-statistics_unilend-performances-gross-data-title {% endtrans %}</h3>
                <div class="table-scroll">

                    <table class="table table-stats-unilend">
                        <thead>
                        <tr>
                            <th class="table-stats-unilend-item-label">&nbsp;</th>
                            {% for year in years %}
                                <th class="table-stats-unilend-item-value">{{ year }}</th>
                            {% endfor %}
                        </tr>
                        </thead>
                        <tbody>
                        {% for label, years in data.regulatoryTable %}
                            {% if label == 'IRR' %}
                                    <tr>
                                        <td class="table-stats-unilend-item-label">{{ "about-us-statistics_regulatory-table-#{label}"|trans|raw }}</td>
                                        {% for value in years %}
                                            <td class="table-stats-unilend-item-value">{{ value|localizednumber}} %</td>
                                        {% endfor %}
                                    </tr>
                            {% elseif label == 'projects' %}
                                    <tr>
                                        <td class="table-stats-unilend-item-label">{{ "about-us-statistics_regulatory-table-#{label}"|trans|raw }}</td>
                                        {% for value in years %}
                                            <td class="table-stats-unilend-item-value">{{ value|localizednumber}}</td>
                                        {% endfor %}
                                    </tr>
                            {% elseif label == 'nb' %}
                                {% for labelNb, yearNb in years %}
                                    <tr>
                                        <td class="table-stats-unilend-item-label">{{ "about-us-statistics_regulatory-table-#{labelNb}"|trans|raw }}</td>
                                        {% for valueNb in yearNb %}
                                            <td class="table-stats-unilend-item-value">{{ valueNb|localizednumber}}</td>
                                        {% endfor %}
                                    </tr>
                                {% endfor %}
                            {% elseif label == 'empty-line' %}
                                <tr><td class="table-stats-unilend-item-label"></td><td class="table-stats-unilend-item-value"></td></tr>
                            {% elseif label == 'pct' %}
                                {% for labelPct, yearPct in years %}
                                    <tr>
                                        <td class="table-stats-unilend-item-label">{{ "about-us-statistics_regulatory-table-#{labelPct}"|trans|raw }}</td>
                                        {% for valuePct in yearPct %}
                                            <td class="table-stats-unilend-item-value">{{ valuePct|localizednumber}} %</td>
                                        {% endfor %}
                                    </tr>
                                {% endfor %}
                            {% else %}
                                <tr>
                                    <td class="table-stats-unilend-item-label">{{ "about-us-statistics_regulatory-table-#{label}"|trans|raw }}</td>
                                    {% for value in years %}
                                        <td class="table-stats-unilend-item-value">{{ value|round|localizednumber }} €</td>
                                    {% endfor %}
                                </tr>
                            {% endif %}
                        {% endfor %}
                        {% set incidenceRate = getStatistic('incidenceRate') %}
                        <tr>
                            <td class="table-stats-unilend-item-label" colspan="4">{% trans %} about-us-statistics_regulatory-table-incidence-rate-amount {% endtrans %}</td>
                            <td class="table-stats-unilend-item-value">{{ incidenceRate['amountIFP']|localizednumber }}%</td>
                        </tr>
                        <tr>
                            <td class="table-stats-unilend-item-label" colspan="4">{% trans %} about-us-statistics_regulatory-table-incidence-rate-projects {% endtrans %}</td>
                            <td class="table-stats-unilend-item-value">{{ incidenceRate['projectsIFP']|localizednumber }}%</td>
                        </tr>
                        </tbody>
                    </table>
                    <p class="field-help text-align-left">{% trans %} about-us-statistics_regulatory-table-default-explanation {% endtrans %}</p>
                </div><!-- /.table-scroll -->
            </div>
        </div><!-- /.block-stats -->

        <div class="btn-toolbar">
            <a href="{{ path('projects_list') }}" class="btn-primary">{% trans %} about-us-statistics_link-to-project-list {% endtrans %}</a>
        </div><!-- /.btn-toolbar -->
    </div><!-- /.site-wrap -->
</section><!-- /#section-platform -->
