<section id="section-intro" class="page-section">
    <div class="site-wrap">
        <div class="row">
            <div class="col-lg-6 col-md-6">
                <header class="block-header">
                    <h1 class="h4">{% trans %} about-us-statistics_general-description-title {% endtrans %}</h1>
                </header>
            </div>
            <div class="col-lg-6 col-md-6">
                <div class="block-text">
                    {% trans with {'%numberProjectRequests%' : getStatistic('numberOfProjectRequests'), '%percentageAcceptedRequests%' : getStatistic('percentageOfAcceptedProjects') }%} about-us-statistics_general-description-text {% endtrans %}
                    <p><a href="{% trans %} about-us-statistics_link-under-general-description-target-url {% endtrans %}" class="link">
                            {% trans %} about-us-statistics_link-under-general-description-text {% endtrans %}</a>
                    </p>
                </div>
            </div>
        </div>
    </div><!-- /.site-wrap -->
</section><!-- /.page-section -->

<div class="bg-bg3">
    <section id="section-preteurs" class="page-section">
        <div class="site-wrap">
            <div class="block-stats block-stats-p1">
                <header class="block-stats-header">
                    <h2>{% trans %} about-us-statistics_lender-statistics-title {% endtrans %}</h2>
                </header>
                <div class="block-promos row readable-wrap bg-icon-linechart">
                    {% set LineChartValue1 = getStatistic('unilendIRR')|round(1, 'floor')|localizednumber %}
                    {% set LineChartValue2 = getStatistic('averageInterestRateForLenders')|round(1, 'floor')|localizednumber %}
                    <div class="promo col-lg-6 col-md-6 col-sm-4">
                        <h3 class="promo-highlight promo-highlight-lg">
                            <span data-textcount data-start-count="0" data-end-count="{{ LineChartValue1 }}" data-total-time="3000" data-limit-decimal="1" data-tween-count="easeOutCirc">
                                {{ LineChartValue1 }}</span><sup>%</sup>
                        </h3>
                        <p>{% trans %} about-us-statistics_lender-statistics-linechart-unilend-irr-text {% endtrans %}</p>
                    </div><!-- /.promo -->
                    <div class="promo col-lg-6 col-md-6 col-sm-4">
                        <h3 class="promo-highlight promo-highlight-lg">
                                <span data-textcount data-start-count="0" data-end-count="{{ LineChartValue2 }}" data-total-time="3000" data-limit-decimal="1" data-tween-count="easeOutCirc">
                                    {{ LineChartValue2 }}</span><sup>%</sup>
                        </h3>
                        <p>{% trans %} about-us-statistics_lender-statistics-linechart-avg-interest-rate-text {% endtrans %}</p>
                    </div><!-- /.promo -->
                </div><!-- /.promos -->
                <div class="stats-charts row">
                    <figure class="stats-chart stats-map">
                        <div class="chart" data-chartview='{{ placeholderData.charts.preterStatisticsMap|json_encode|escape('html_attr') }}'>
                            {# @todo Chart #}
                            {# <img src="{{ siteurlmedia('apropos/statistiques/map-fr-repartition.png') }}" alt="" /> #}
                        </div>
                        <figcaption><p>{% trans %} about-us-statistics_lender-statistics-map-title {% endtrans %}</p></figcaption>
                    </figure>
                    <div class="stats-members">
                        <img src="{{ siteurlmedia('apropos/statistiques/icons-members.png') }}" alt="">
                        <div>
                            <span class="stats-number"><span data-textcount data-start-count="0" data-total-time="3000" data-tween-count="easeOutCirc">{{ getStatistic('numberOfLendersInCommunity')|localizednumber }}</span></span> {% trans %} about-us-statistics_lender-statistics-members-active-not-text {% endtrans %}
                        </div>
                        <div>
                            <span class="stats-number"><span data-textcount data-start-count="0" data-total-time="3000" data-tween-count="easeOutCirc">{{ getStatistic('numberOfActiveLenders')|localizednumber }}</span></span> {% trans %} about-us-statistics_lender-statistics-members-type-text {% endtrans %}
                        </div>
                    </div>
                    {% set lendersByType = getStatistic('lendersByType') %}
                    <div class="stats-enterprises">
                        <img src="{{ siteurlmedia('apropos/statistiques/icons-enterprises.png') }}" alt="">
                        <div>
                            <span class="stats-number"><span data-textcount data-start-count="0" data-total-time="3000" data-tween-count="easeOutCirc">{{ lendersByType.person.percentage|localizednumber }}</span><sup>%</sup></span> {% trans %} about-us-statistics_lender-statistics-members-type-person-text {% endtrans %}
                        </div>
                        <div>
                            <span class="stats-number">{{ lendersByType.legalEntity.percentage|localizednumber }}<sup>%</sup></span>
                            {% trans %} about-us-statistics_lender-statistics-members-type-legal-entity-text {% endtrans %}</div>
                    </div>
                </div><!-- /.stats-map -->
                <div class="block-promos row">
                    <div class="promo col-lg-4 col-md-4">
                        <h3 class="promo-highlight promo-highlight-lg">
                            <span data-textcount data-start-count="0" data-total-time="3000" data-tween-count="easeOutCirc">{{ getStatistic('averageNumberOfLenders')|round|localizednumber }}</span>
                        </h3>
                        <p>{% trans %} about-us-statistics_lender-statistics-1-text {% endtrans %}</p>
                    </div><!-- /.promo -->
                    <div class="promo col-lg-4 col-md-4">
                        <h3 class="promo-highlight promo-highlight-lg">
                            <span data-textcount data-start-count="0" data-total-time="3000" data-tween-count="easeOutCirc">{{ getStatistic('averageLoanAmount')|round|localizednumber }}</span><sup>€</sup>
                        </h3>
                        <p>{% trans %} about-us-statistics_lender-statistics-2-text {% endtrans %}</p>
                    </div><!-- /.promo -->
                    <div class="promo col-lg-4 col-md-4">
                        <h3 class="promo-highlight promo-highlight-lg">
                            <span data-textcount data-start-count="0" data-total-time="3000" data-tween-count="easeOutCirc">TODO</span>
                            {#TODO#}
                        </h3>
                        <p>{% trans %} about-us-statistics_lender-statistics-3-text {% endtrans %}</p>
                    </div><!-- /.promo -->
                </div><!-- /.promos -->
            </div><!-- /.block-stats -->
        </div>
    </section><!-- /#section-preteurs -->

