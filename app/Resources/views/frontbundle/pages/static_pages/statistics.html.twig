{% set projectsByCategoryChart = {
    type: 'preterProjectsCategories',
    background: 'none',
    highcharts: {
        chart: {
            type: 'treemap',
            spacing: [0, 0, 0, 0]
            },
        title: {
        text: ''
        },
        plotOptions: {
            treemap: {
                borderColor: '#B40066',
                borderWidth: 2,
                dataLabels: {
                    useHTML: true,
                    zIndex: -1
                }
            }
        },
        tooltip: {
            backgroundColor: "rgba(255,255,255,1)",
            useHTML: true
        },
        series: [{
            type: 'treemap',
            layoutAlgorithm: 'squarified',
            data: data.projectCountForCategoryTreeMap
