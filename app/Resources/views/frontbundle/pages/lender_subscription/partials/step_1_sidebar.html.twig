<div class="col-promos col-lg-4 col-md-4">
    <div class="promo">
        <div class="promo-image">
            {{ svgimage('#promo-francemap', '', 190, 190, 'none') | raw }}
        </div>
        <h3 class="promo-highlight">
            <span class="ui-text-count" data-start-count="0" data-end-count="{{ 'lender-subscription_step-1-statistics-francemap-value'|trans }}" data-total-time="3000" data-round-float="round" data-tween-count="easeOutCirc">{{ 'lender-subscription_step-1-statistics-francemap-value'|trans }}</span><sup>%</sup>
        </h3>
        <p>{% trans %} lender-subscription_step-1-statistics-francemap-text {% endtrans %}</p>
    </div><!-- /.promo -->

    <div class="promo">
        <div class="promo-image">
            {{ svgimage('#promo-linechart2', '', 190, 190, 'none') | raw }}
        </div>
        {% if 'lender-subscription_step-2-statistics-section-linechart-value'|trans != 'lender-subscription_step-2-statistics-section-linechart-value' %}
            {% set valueLineChart = getStatistic('averageInterestRateForLenders')|round(1, 'floor')|localizednumber %}
        {% else %}
            {% set valueLineChart = 'lender-subscription_step-2-statistics-section-linechart-value'|trans %}
        {% endif %}
        <h3 class="promo-highlight">
            <span class="ui-text-count" data-start-count="0" data-end-count="{{ valueLineChart }}" data-total-time="3000" data-limit-decimal="1" data-tween-count="easeOutCirc">
                {{ valueLineChart }}
            </span>
            <sup>%</sup>
        </h3>
        <p>{% trans with {'%avgYearlyInterestRate%' : valueLineChart }%} lender-subscription_step-1-statistics-linechart2-text {% endtrans %}</p>
    </div><!-- /.promo -->

    <div class="promo">
        <div class="promo-image">
            {{ svgimage('#promo-clock', '', 190, 190, 'none') | raw }}
        </div>
        <h3 class="promo-highlight">{% trans %} lender-subscription_step-1-statistics-clock-value {% endtrans %}</h3>
        <p>{% trans %} lender-subscription_step-1-statistics-clock-text {% endtrans %}</p>
    </div><!-- /.promo -->
</div>