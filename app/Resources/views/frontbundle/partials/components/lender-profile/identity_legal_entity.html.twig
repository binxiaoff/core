<section id="profile-info-view" role="tabpanel" class="profile-info-view collapse in">
    <a href="#profile-info-edit" role="button" data-toggle="collapse" data-parent="#profile-info-section" class="btn-edit" aria-controls="#profile-info-edit" aria-expanded="false">
        <span class="icon fa-pencil-u32"></span>
        <span class="label">{% trans %} lender-profile_information-tab-identity-section-modify-button {% endtrans %}</span></a>
    <dl class="list-2col">
        <dt>{% trans %} lender-profile_information-tab-identity-section-client-type-label {% endtrans %}</dt>
        <dd id="profile-info-statut-output">{% trans %} lender-profile_information-tab-identity-section-client-type-legal-entity-label {% endtrans %}</dd>
        <dt>{% trans %} lender-profile_information-tab-identity-section-company-name-label {% endtrans %}</dt>
        <dd id="profile-info-company-name-output">{{ company.name }}</dd>
        <dt>{% trans %} lender-profile_information-tab-identity-section-company-legal-form-label {% endtrans %}</dt>
        <dd id="profile-info-company-form-output">{{ company.forme }}</dd>
        <dt>{% trans %} lender-profile_information-tab-identity-section-company-social-capital-label {% endtrans %}</dt>
        <dd id="profile-info-company-name-output">{{ company.capital }}</dd>
        <dt>{% trans %} lender-profile_information-tab-identity-section-company-siren-label {% endtrans %}</dt>
        <dd id="profile-info-company-form-output">{{ company.siren }}</dd>
        <dt>{% trans %} lender-profile_information-tab-identity-section-company-siren-label {% endtrans %}</dt>
        <dd id="profile-info-company-phone-output">{{ company.phone }}</dd>
    </dl>
    <h5>Identité du representant</h5>
    <dl class="list-2col">
    <dt>{% trans %} lender-profile_information-tab-identity-section-form-of-address-label {% endtrans %}</dt>
        {% if client.civilite == 'M.' %}
            <dd id="profile-info-civilite-output">{% trans %} lender-profile_information-tab-identity-section-form-of-address-male-label {% endtrans %}</dd>
        {% else %}
            <dd id="profile-info-civilite-output">{% trans %} lender-profile_information-tab-identity-section-form-of-address-female-label {% endtrans %}</dd>
        {% endif %}
        <dt>{% trans %} lender-profile_information-tab-identity-section-company-client-status-label {% endtrans %}</dt>
        <dd>{{ "lender-profile_information-tab-identity-section-company-client-status-#{ company.status_client }" | trans }}
            {% if company.status_client == 3 %}
                {% if company.status_conseil_externe_entreprise == 3 %}
                    {{ company.preciser_conseil_externe_entreprise }}
                {% else %}
                    {{ externalCounselList[company.status_conseil_externe_entreprise] }}
                {% endif %}
            {% endif %}
        </dd>
        <dt>{% trans %} lender-profile_information-tab-identity-section-name-label {% endtrans %}</dt>
        <dd id="profile-info-nom-output">{{ client.nom }}</dd>
        <dt>{% trans %} lender-profile_information-tab-identity-section-first-name-label {% endtrans %}</dt>
        <dd id="profile-info-prenom-output">{{ client.prenom }}</dd>

        <dt>{% trans %} lender-profile_information-tab-identity-section-company-client-function-label {% endtrans %}</dt>
        <dd id="profile-info-dob-output">{{ client.fonction }}</dd>
    </dl>
    {% if company.status_client > 1 %}
    <h5>Identité du dirigant</h5>
    <dl class="list-2col">
        <dt>{% trans %} lender-profile_information-tab-identity-section-form-of-address-label {% endtrans %}</dt>
        {% if company.civilite_dirigeant == 'M.' %}
            <dd id="profile-info-civilite-output">{% trans %} lender-profile_information-tab-identity-section-form-of-address-male-label {% endtrans %}</dd>
        {% else %}
            <dd id="profile-info-civilite-output">{% trans %} lender-profile_information-tab-identity-section-form-of-address-female-label {% endtrans %}</dd>
        {% endif %}
        <dt>{% trans %} lender-profile_information-tab-identity-section-name-label {% endtrans %}</dt>
        <dd id="profile-info-nom-output">{{ company.nom_dirigeant }}</dd>
        <dt>{% trans %} lender-profile_information-tab-identity-section-first-name-label {% endtrans %}</dt>
        <dd id="profile-info-prenom-output">{{ company.prenom_dirigeant }}</dd>
        <dt>{% trans %} lender-profile_information-tab-identity-section-company-client-function-label {% endtrans %}</dt>
        <dd id="profile-info-dob-output">{{ company.fonction_dirigeant }}</dd>
        <dt>{% trans %} Téléphone {% endtrans %}</dt>
        <dd id="profile-info-dob-output">{{ company.phone_dirigeant }}</dd>
        <dt>{% trans %} Email {% endtrans %}</dt>
        <dd id="profile-info-dob-output">{{ company.email_dirigeant }}</dd>
    </dl>
    {% endif %}
    <dl class="list-2col">
    <dt>{% trans %} lender-profile_information-tab-identity-section-ID-documents-label {% endtrans %}</dt>
        <dd id="profile-info-fichier-identitie-output">
        {% for attachment in companyIdAttachments %}
            {% if attachment.id_type == constant('\\attachment_type::CNI_PASSPORTE_DIRIGEANT') %}
                <span id="form-profile-info-file-identite-cni-1-output" class="file-justificatif file-identity-side-a"
                      title="{{ "lender-attachement-provided-label-type-#{ attachment.id_type }" | transchoice(attachment.added|date('d/m-Y')) }}" data-toggle="tooltip">
                    <span class="sr-only">{{ "lender-attachement-provided-label-type-#{ attachment.id_type }" | transchoice(attachment.added|date('d/m-Y'))}}</span>
                </span>
                {% elseif attachment.id_type == constant('\\attachment_type::CNI_PASSPORTE_VERSO') %}
                    <span id="form-profile-info-file-identite-cni-2-output" class="file-justificatif file-identity-side-b"
                          title="{{ "lender-attachement-provided-label-type-#{ attachment.id_type }" | transchoice(attachment.added|date('d/m-Y')) }}" data-toggle="tooltip">
                        <span class="sr-only">{{ "lender-attachement-provided-label-type-#{ attachment.id_type }" | transchoice(attachment.added|date('d/m-Y'))}}</span>
                    </span>
            {% endif %}
        {% endfor %}
        </dd>
        <dt>{% trans %} Autres justificatifs {% endtrans %}</dt>
        <dd id="profile-info-fichier-domicile-output">
            {% for attachment in companyOtherAttachments  %}
                <span id="form-profile-address-file-domicile-1-output" class="file-justificatif file-domicile"
                      title="{{ "lender-attachement-provided-label-type-#{ attachment.id_type }" | transchoice(attachment.added|date('d/m/Y')) }}" data-toggle="tooltip">
                    <span class="sr-only">{{ "lender-attachement-provided-label-type-#{ attachment.id_type }" | transchoice(attachment.added|date('d/m/Y')) }}</span>
                </span>
            {% endfor %}
        </dd>
    </dl>
</section>
