<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
    <bean id="preteurs" class="com.salesforce.dataloader.process.ProcessRunner" singleton="false">
        <description>Inserts Lenders from CSV file.</description>
        <property name="name" value="preteurs"/>
        <property name="configOverrideMap">
            <map>
                <entry key="sfdc.debugMessages" value="false"/>
                <entry key="sfdc.debugMessagesFile" value="/data/log/unilend/dataloader/lenders.log"/>
                <entry key="sfdc.endpoint" value="https://www.salesforce.com"/>
                <entry key="sfdc.username" value="arnaud.schwartz@unilend.fr"/>
                <entry key="sfdc.password" value="164ef5989caaf601fec5fe26a6b5d333d3e99418a4c8d17e6019657fa5e0a49418459d78e43eeb22"/>
                <entry key="process.encryptionKeyFile" value="/data/sites/unilend/app/dataloader/conf/key.txt"/>
                <entry key="sfdc.timeoutSecs" value="540"/>
                <entry key="sfdc.loadBatchSize" value="200"/>
                <entry key="sfdc.entity" value="Contact"/>
                <entry key="sfdc.externalIdField" value="ID_Client__c"/>
                <entry key="sfdc.insertNulls" value="true"/>
                <entry key="process.operation" value="upsert"/>
                <entry key="process.mappingFile" value="/data/sites/unilend/app/dataloader/map/Mapping_Lenders.sdl"/>
                <entry key="process.outputError" value="/data/log/dataloader/errorLenders.csv"/>
                <entry key="dataAccess.name" value="/data/medias/nfs/protected/dataloader/preteurs.csv" />
                <entry key="dataAccess.type" value="csvRead" />
                <entry key="dataAccess.readUTF8" value="true" />
                <entry key="process.initialLastRunDate" value="2015-09-17T00:00:00.000-0800"/>
                <entry key="process.useEuropeanDates" value="true"/>
            </map>
        </property>
    </bean>
