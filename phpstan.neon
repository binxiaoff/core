includes:
    - vendor/phpstan/phpstan-doctrine/extension.neon
    - vendor/phpstan/phpstan-symfony/extension.neon
    - vendor/phpstan/phpstan-phpunit/extension.neon
    - vendor/phpstan/phpstan-phpunit/rules.neon

parameters:
    level: 0
    paths:
        - src
        - tests
    autoload_files:
        - %rootDir%/../../../bin/.phpunit/phpunit-8.3-0/vendor/autoload.php
    excludes_analyse:
        - %rootDir%/../../../src/Migrations
    symfony:
        container_xml_path: %rootDir%/../../../var/cache/dev/Unilend_KernelDevDebugContainer.xml
