{
  "name": "unilend/application",
  "license": "proprietary",
  "type": "project",
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Unilend\\": "src/",
      "Unilend\\core\\": "apps/core/",
      "Unilend\\Test\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.3.6",
    "ext-bcmath": "*",
    "ext-ctype": "*",
    "ext-curl": "*",
    "ext-hash": "*",
    "ext-openssl": "*",
    "ext-pdo": "*",
    "ext-soap": "*",
    "ext-zip": "*",
    "azuyalabs/yasumi": "^2.0",
    "beberlei/doctrineextensions": "^1.0",
    "cravler/maxmind-geoip-bundle": "^1.2",
    "docusign/esign-client": "^3.0",
    "eightpoints/guzzle-bundle": "^7.2",
    "facile-it/doctrine-mysql-come-back": "^1.6",
    "friendsofsymfony/jsrouting-bundle": "^2.3",
    "gushphp/to-inline-style-email-bundle": "^1.4",
    "jbroadway/urlify": "^1.0",
    "knplabs/knp-snappy-bundle": "^1.5",
    "lcobucci/jwt": "^3.2",
    "mailjet/mailjet-apiv3-php": "^1.1",
    "misd/phone-number-bundle": "^1.3",
    "nexylan/slack-bundle": "^2.0",
    "phpoffice/phpspreadsheet": "^1.3",
    "piwik/device-detector": "^3.11",
    "ramsey/uuid": "^3.7",
    "scienta/doctrine-json-functions": "^4.0",
    "sensio/framework-extra-bundle": "^5.2",
    "sensiolabs/security-checker": "^6.0",
    "sonata-project/cache-bundle": "^3.0",
    "stof/doctrine-extensions-bundle": "^1.3",
    "symfony/asset": "*",
    "symfony/cache": "*",
    "symfony/config": "*",
    "symfony/console": "*",
    "symfony/contracts": "^1.1",
    "symfony/debug": "*",
    "symfony/dependency-injection": "*",
    "symfony/doctrine-bridge": "*",
    "symfony/dotenv": "*",
    "symfony/event-dispatcher": "*",
    "symfony/filesystem": "*",
    "symfony/finder": "*",
    "symfony/flex": "^1.2",
    "symfony/form": "*",
    "symfony/framework-bundle": "*",
    "symfony/http-client": "*",
    "symfony/http-foundation": "*",
    "symfony/http-kernel": "*",
    "symfony/lock": "*",
    "symfony/messenger": "*",
    "symfony/monolog-bundle": "^3.1",
    "symfony/options-resolver": "*",
    "symfony/orm-pack": "^1.0",
    "symfony/proxy-manager-bridge": "*",
    "symfony/routing": "*",
    "symfony/security": "*",
    "symfony/security-bundle": "*",
    "symfony/stopwatch": "*",
    "symfony/swiftmailer-bundle": "^3.1",
    "symfony/templating": "*",
    "symfony/translation": "*",
    "symfony/twig-bundle": "*",
    "symfony/validator": "*",
    "symfony/yaml": "*",
    "thadafinser/user-agent-parser": "^2.0",
    "twig/extensions": "^1.5",
    "whichbrowser/parser": "^2.0",
    "xynnn/google-tag-manager-bundle": "^2.0"
  },
  "require-dev": {
    "escapestudios/symfony2-coding-standard": "^3.5",
    "friendsofphp/php-cs-fixer": "^2.14",
    "phpmd/phpmd": "^2.6",
    "phpro/grumphp": "^0.15",
    "phpstan/phpstan-doctrine": "^0.11",
    "phpstan/phpstan-phpunit": "^0.11",
    "phpstan/phpstan-symfony": "^0.11",
    "squizlabs/php_codesniffer": "^3.4",
    "symfony/browser-kit": "*",
    "symfony/debug-bundle": "*",
    "symfony/maker-bundle": "^1.0",
    "symfony/test-pack": "^1.0",
    "symfony/web-profiler-bundle": "*"
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "public-dir": "public/default",
    "symfony": {
      "allow-contrib": true,
      "require": "4.3.*"
    }
  }
}
