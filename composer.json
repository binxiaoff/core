{
  "name": "unilend/application",
  "description": "KLS API application",
  "license": "proprietary",
  "type": "project",
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Unilend\\": "src/",
      "Unilend\\Test\\": "tests/",
      "PHPUnit\\": "bin/.phpunit/phpunit"
    }
  },
  "require": {
    "php": ">=7.4.7",
    "ext-bcmath": "*",
    "ext-ctype": "*",
    "ext-curl": "*",
    "ext-dom": "*",
    "ext-hash": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "ext-pdo": "*",
    "ext-simplexml": "*",
    "ext-soap": "*",
    "ext-zip": "*",
    "ext-zlib": "*",
    "api-platform/api-pack": "^1.2",
    "api-platform/core": "^2.6",
    "azuyalabs/yasumi": "^2.0",
    "beberlei/doctrineextensions": "^1.0",
    "cravler/maxmind-geoip-bundle": "^1.5",
    "defuse/php-encryption": "^2.2",
    "doctrine/orm": "^2.7",
    "docusign/esign-client": "^3.0",
    "eightpoints/guzzle-bundle": "^8.1",
    "enlightn/security-checker": "^1.4",
    "enqueue/sqs": "^0.10.1",
    "exercise/htmlpurifier-bundle": "^3.0",
    "facile-it/doctrine-mysql-come-back": "^1.6",
    "friendsofsymfony/jsrouting-bundle": "^2.3",
    "gesdinet/jwt-refresh-token-bundle": "^0.10",
    "http-interop/http-factory-guzzle": "^1.0",
    "jbroadway/urlify": "^1.0",
    "knplabs/knp-snappy-bundle": "^1.5",
    "lcobucci/jwt": "^3.2",
    "league/flysystem-aws-s3-v3": "1.0.25",
    "lexik/jwt-authentication-bundle": "^2.6",
    "nexylan/slack-bundle": "^2.0",
    "odolbeau/phone-number-bundle": "^3.1",
    "oneup/flysystem-bundle": "^3.1",
    "php-http/guzzle6-adapter": "^2.0",
    "piwik/device-detector": "^3.11",
    "ramsey/uuid": "^3.7",
    "robrichards/xmlseclibs": "^3.0",
    "rollbar/rollbar-php-symfony-bundle": "^4.0",
    "scienta/doctrine-json-functions": "^4.0",
    "sensio/framework-extra-bundle": "^5.2",
    "sroze/messenger-enqueue-transport": "^0.5.0",
    "stof/doctrine-extensions-bundle": "^1.3",
    "symfony/asset": "5.2.*",
    "symfony/cache": "5.2.*",
    "symfony/config": "5.2.*",
    "symfony/console": "5.2.*",
    "symfony/contracts": "^2.1",
    "symfony/dependency-injection": "5.2.*",
    "symfony/doctrine-bridge": "5.2.*",
    "symfony/dotenv": "5.2.*",
    "symfony/event-dispatcher": "5.2.*",
    "symfony/filesystem": "5.2.*",
    "symfony/finder": "5.2.*",
    "symfony/flex": "^1.8",
    "symfony/form": "5.2.*",
    "symfony/framework-bundle": "5.2.*",
    "symfony/http-client": "5.2.*",
    "symfony/http-foundation": "5.2.*",
    "symfony/http-kernel": "5.2.*",
    "symfony/lock": "5.2.*",
    "symfony/messenger": "5.2.*",
    "symfony/mime": "5.2.*",
    "symfony/monolog-bundle": "^3.1",
    "symfony/options-resolver": "5.2.*",
    "symfony/orm-pack": "^2.1",
    "symfony/proxy-manager-bridge": "5.2.*",
    "symfony/routing": "5.2.*",
    "symfony/security-bundle": "5.2.*",
    "symfony/stopwatch": "5.2.*",
    "symfony/string": "5.2.*",
    "symfony/swiftmailer-bundle": "^3.1",
    "symfony/templating": "5.2.*",
    "symfony/translation": "5.2.*",
    "symfony/twig-bundle": "5.2.*",
    "symfony/validator": "5.2.*",
    "symfony/yaml": "5.2.*",
    "whichbrowser/parser": "^2.0"
  },
  "require-dev": {
    "dama/doctrine-test-bundle": "^6.4",
    "dg/bypass-finals": "^1.1",
    "doctrine/doctrine-fixtures-bundle": "^3.3",
    "escapestudios/symfony2-coding-standard": "^3.5",
    "friendsofphp/php-cs-fixer": "^2.14",
    "fzaninotto/faker": "^1.9",
    "hautelook/alice-bundle": "^2.5",
    "pheromone/phpcs-security-audit": "^2.0",
    "phpmd/phpmd": "^2.6",
    "phpro/grumphp": "^1.2.0",
    "phpspec/prophecy": "^1.8",
    "phpstan/phpstan": "^0.12",
    "phpstan/phpstan-doctrine": "^0.12",
    "phpstan/phpstan-phpunit": "^0.12",
    "phpstan/phpstan-symfony": "^0.12",
    "squizlabs/php_codesniffer": "^3.4",
    "symfony/browser-kit": "5.2.*",
    "symfony/debug-bundle": "5.2.*",
    "symfony/maker-bundle": "^1.0",
    "symfony/test-pack": "^1.0",
    "symfony/web-profiler-bundle": "5.2.*"
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-mbstring": "*",
    "symfony/polyfill-intl-icu": "*",
    "symfony/polyfill-intl-idn": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd",
      "security-checker security:check": "script"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "public-dir": "public",
    "symfony": {
      "allow-contrib": true,
      "require": "5.2.*"
    }
  }
}
