{
  "name": "unilend/site_project",
  "license": "proprietary",
  "type": "project",
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Unilend\\": "src/",
      "Unilend\\core\\": "core/",
      "Unilend\\Test\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.1.11",
    "ext-bcmath": "*",
    "ext-curl": "*",
    "ext-openssl": "^7.1",
    "ext-pdo": "*",
    "ext-soap": "*",
    "alcaeus/mongo-php-adapter": "^1.1",
    "azuyalabs/yasumi": "^2.0",
    "beberlei/doctrineextensions": "^1.0",
    "box/spout": "^2.7",
    "cache/adapter-bundle": "^1.1",
    "cache/memcache-adapter": "^1.0",
    "cravler/maxmind-geoip-bundle": "^1.2",
    "doctrine/mongodb-odm-bundle": "^3.4",
    "docusign/esign-client": "^3.0",
    "eightpoints/guzzle-bundle": "^7.2",
    "ekino/newrelic-bundle": "^2.0",
    "facile-it/doctrine-mysql-come-back": "^1.6",
    "gushphp/to-inline-style-email-bundle": "^1.4",
    "hashids/hashids": "^3.0",
    "incenteev/composer-parameter-handler": "^2.0",
    "jbroadway/urlify": "^1.0",
    "jms/serializer-bundle": "^2.2",
    "knplabs/knp-snappy-bundle": "^1.5",
    "lcobucci/jwt": "^3.2",
    "mailjet/mailjet-apiv3-php": "^1.1",
    "monext/payline-sdk": "^4.53",
    "mpdf/mpdf": "^8.0",
    "nexylan/slack-bundle": "^2.0",
    "php-http/guzzle6-adapter": "^2.0",
    "phpoffice/phpexcel": "1.8.*",
    "phpoffice/phpspreadsheet": "^1.3",
    "phpxmlrpc/phpxmlrpc": "^4.3",
    "piwik/device-detector": "^3.11",
    "ramsey/uuid": "^3.7",
    "scienta/doctrine-json-functions": "^4.0",
    "sensio/framework-extra-bundle": "^5.2",
    "sensiolabs/security-checker": "^5.0",
    "sonata-project/cache-bundle": "^3.0",
    "sonata-project/seo-bundle": "^2.0",
    "symfony-cmf/routing-bundle": "^2.0",
    "symfony/asset": "*",
    "symfony/cache": "*",
    "symfony/config": "*",
    "symfony/console": "*",
    "symfony/debug": "*",
    "symfony/dependency-injection": "*",
    "symfony/event-dispatcher": "*",
    "symfony/filesystem": "*",
    "symfony/finder": "*",
    "symfony/flex": "^1.2",
    "symfony/form": "*",
    "symfony/framework-bundle": "*",
    "symfony/http-client": "^4.3@dev",
    "symfony/http-foundation": "*",
    "symfony/http-kernel": "*",
    "symfony/lock": "*",
    "symfony/monolog-bundle": "^3.1",
    "symfony/options-resolver": "*",
    "symfony/orm-pack": "^1.0",
    "symfony/proxy-manager-bridge": "*",
    "symfony/routing": "*",
    "symfony/security": "*",
    "symfony/security-bundle": "*",
    "symfony/stopwatch": "*",
    "symfony/swiftmailer-bundle": "^3.1",
    "symfony/templating": "*",
    "symfony/translation": "*",
    "symfony/twig-bundle": "*",
    "symfony/validator": "*",
    "symfony/yaml": "*",
    "thadafinser/user-agent-parser": "^2.0",
    "twig/extensions": "^1.5",
    "whichbrowser/parser": "^2.0",
    "xynnn/google-tag-manager-bundle": "^2.0"
  },
  "require-dev": {
    "escapestudios/symfony2-coding-standard": "^3.5",
    "friendsofphp/php-cs-fixer": "^2.14",
    "phpmd/phpmd": "^2.6",
    "phpro/grumphp": "^0.15.0",
    "phpstan/phpstan-doctrine": "^0.11.2",
    "phpstan/phpstan-phpunit": "0.11",
    "phpstan/phpstan-symfony": "^0.11.3",
    "squizlabs/php_codesniffer": "^3.4",
    "symfony/browser-kit": "*",
    "symfony/debug-bundle": "*",
    "symfony/dotenv": "*",
    "symfony/test-pack": "1.0.5",
    "symfony/web-profiler-bundle": "*"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "public-dir": "public/default",
    "symfony": {
      "allow-contrib": true,
      "require": "4.3.*"
    }
  }
}
