{
  "name": "unilend/site_project",
  "license": "proprietary",
  "type": "project",
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Unilend\\": "",
      "Unilend\\Bridge\\": "src/Unilend/Bridge",
      "Unilend\\Bundle\\": "src/Unilend/Bundle"
    },
    "classmap": [
      "app/AppKernel.php",
      "app/AppCache.php"
    ]
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/ipecador/KnpMailjetBundle.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/ipecador/mailjet-api-php.git"
    }
  ],
  "require": {
    "php": ">=7.1.11",
    "knplabs/knp-snappy-bundle": "^1.5",
    "phpxmlrpc/phpxmlrpc": "^4.3",
    "phpoffice/phpexcel": "1.8.*",
    "mailjet/mailjet-apiv3-php": "^1.1",
    "symfony/symfony": "3.4.*",
    "symfony/swiftmailer-bundle": "^3.1",
    "symfony/monolog-bundle": "^3.1",
    "doctrine/doctrine-bundle": "^1.8",
    "doctrine/orm": "^2.5",
    "cache/adapter-bundle": "^1.1",
    "cache/memcache-adapter": "^1.0",
    "sensio/distribution-bundle": "^5.0",
    "sensio/framework-extra-bundle": "^5.1",
    "incenteev/composer-parameter-handler": "^2.0",
    "facile-it/doctrine-mysql-come-back": "^1.6",
    "symfony-cmf/routing-bundle": "^2.0",
    "twig/extensions": "^1.5",
    "xynnn/google-tag-manager-bundle": "^2.0",
    "sonata-project/seo-bundle": "^2.0",
    "ekino/newrelic-bundle": "^1.4",
    "sonata-project/cache-bundle": "^2.3",
    "knplabs/mailjet-api-php": "v2.x-dev",
    "knplabs/knp-mailjet-bundle": "v2.x-dev",
    "cleentfaar/slack-bundle": "^0.20.1",
    "ramsey/uuid": "^3.7",
    "jbroadway/urlify": "^1.0",
    "doctrine/mongodb-odm-bundle": "^3.4",
    "beberlei/DoctrineExtensions": "^1.0",
    "jms/serializer-bundle": "^2.2",
    "eightpoints/guzzle-bundle": "^7.2",
    "box/spout": "^2.7",
    "hashids/hashids": "^2.0",
    "gushphp/to-inline-style-email-bundle": "^1.4",
    "alcaeus/mongo-php-adapter": "^1.1",
    "welp/mailchimp-bundle": "^1.2",
    "doctrine/doctrine-migrations-bundle": "^1.3"
  },
  "require-dev": {
    "sensio/generator-bundle": "^3.0"
  },
  "scripts": {
    "symfony-scripts": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
    ],
    "post-install-cmd": [
      "@symfony-scripts"
    ],
    "post-update-cmd": [
      "@symfony-scripts"
    ]
  },
  "extra": {
    "symfony-app-dir": "app",
    "symfony-bin-dir": "bin",
    "symfony-var-dir": "var",
    "symfony-web-dir": "public/default",
    "symfony-tests-dir": "tests",
    "symfony-assets-install": "relative",
    "incenteev-parameters": {
      "file": "app/config/parameters.yml"
    }
  }
}
