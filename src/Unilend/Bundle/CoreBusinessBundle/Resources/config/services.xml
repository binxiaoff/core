<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <!-- Unilend services -->
        <service id="unilend.service.autobid_settings_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\AutoBidSettingsManager">
            <argument type="service" id="unilend.service.entity_manager"/>
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="unilend.service.client_settings_manager"/>
            <argument type="service" id="unilend.service.client_manager"/>
            <argument type="service" id="unilend.service.notification_manager"/>
            <argument type="service" id="unilend.service.lender_manager"/>
            <argument type="service" id="unilend.service_product.product_manager"/>
            <argument type="service" id="unilend.service_product_contract.contract_manager"/>
        </service>

        <service id="unilend.service.bid_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\BidManager">
            <argument type="service" id="unilend.service.entity_manager"/>
            <argument type="service" id="unilend.service.notification_manager"/>
            <argument type="service" id="unilend.service.autobid_settings_manager"/>
            <argument type="service" id="unilend.service.lender_manager"/>
            <argument type="service" id="unilend.service_product.product_manager"/>
            <argument type="service" id="unilend.service.cip_manager"/>
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="unilend.service.wallet_manager"/>
            <call method="setLogger">
                <argument type="service" id="logger" on-invalid="null"/>
            </call>
        </service>

        <service id="unilend.service.client_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\ClientManager">
            <argument type="service" id="unilend.service.entity_manager"/>
            <argument type="service" id="unilend.service.client_settings_manager"/>
            <argument type="service" id="security.token_storage"/>
            <argument type="service" id="request_stack"/>
            <argument type="service" id="unilend.service.wallet_creation_manager"/>
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="logger"/>
            <argument type="service" id="router"/>
        </service>

        <service id="unilend.service.lender_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\LenderManager">
            <argument type="service" id="unilend.service.entity_manager"/>
            <argument type="service" id="doctrine.orm.entity_manager"/>
        </service>

        <service id="unilend.service.loan_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\LoanManager">
            <argument type="service" id="unilend.service.entity_manager"/>
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <call method="setLogger">
                <argument type="service" id="logger" on-invalid="null"/>
            </call>
        </service>

        <service id="unilend.service.notification_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\NotificationManager">
            <argument type="service" id="unilend.service.entity_manager"/>
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="unilend.service.email_manager"/>
        </service>

        <service id="unilend.service.email_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\MailerManager">
            <argument type="service" id="service_container"/>
            <argument type="service" id="unilend.service.entity_manager"/>
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="unilend.swiftmailer.message_provider"/>
            <argument type="service" id="swiftmailer.mailer.default"/>
            <argument>%kernel.default_locale%</argument>
            <argument type="service" id="assets.packages"/>
            <argument>%router.request_context.scheme%</argument>
            <argument>%url.host_default%</argument>
            <argument>%url.host_admin%</argument>
            <argument type="service" id="translator.default"/>
            <argument type="service" id="logger"/>
        </service>

        <service id="unilend.service.project_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\ProjectManager">
            <argument type="service" id="unilend.service.entity_manager"/>
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="unilend.service.bid_manager"/>
            <argument type="service" id="unilend.service.loan_manager"/>
            <argument type="service" id="unilend.service.notification_manager"/>
            <argument type="service" id="unilend.service.email_manager"/>
            <argument type="service" id="unilend.service.project_rate_settings_manager"/>
            <argument type="service" id="unilend.service_product.product_manager"/>
            <argument type="service" id="unilend.service_product_contract.contract_attribute_manager"/>
            <argument type="service" id="unilend.service.slack_manager"/>
            <argument type="service" id="unilend.service.risk_data_monitoring_manager"/>
            <argument type="service" id="unilend.service.sponsorship_manager"/>
            <argument>%url.universign%</argument>
            <argument type="service" id="unilend.service_repayment.project_repayment_task_manager"/>
            <call method="setLogger">
                <argument type="service" id="logger" on-invalid="null"/>
            </call>
        </service>

        <service id="unilend.service.client_settings_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\ClientSettingsManager">
            <argument type="service" id="unilend.service.entity_manager"/>
            <argument type="service" id="memcache.default"/>
        </service>

        <service id="unilend.service.irr_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\IRRManager">
            <argument type="service" id="unilend.service.entity_manager"/>
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="logger"/>
        </service>

        <service id="unilend.service.entity_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\Simulator\EntityManager">
            <argument type="service" id="database_connection"/>
        </service>

        <service id="unilend.service.project_rate_settings_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\ProjectRateSettingsManager">
            <argument type="service" id="unilend.service.entity_manager"/>
            <argument type="service" id="unilend.swiftmailer.message_provider"/>
            <argument type="service" id="swiftmailer.mailer.default"/>
        </service>

        <service id="unilend.service.salesforce" class="Unilend\Bundle\CoreBusinessBundle\Service\SalesforceManager">
            <argument type="service" id="unilend.service.entity_manager"/>
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument>%path.protected%dataloader</argument>
            <argument>%kernel.root_dir%/dataloader/conf</argument>
            <argument type="service" id="logger"/>
        </service>

        <service id="unilend.service.statistics_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\StatisticsManager">
            <argument type="service" id="unilend.service.entity_manager"/>
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="unilend.service.irr_manager"/>
            <argument type="service" id="memcache.default"/>
            <argument type="service" id="unilend.service.location_manager"/>
        </service>

        <service id="unilend.service.welcome_offer_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\WelcomeOfferManager">
            <argument type="service" id="unilend.service.operation_manager"/>
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="number_formatter" />
            <argument type="service" id="unilend.swiftmailer.message_provider"/>
            <argument type="service" id="swiftmailer.mailer.default"/>
            <argument type="service" id="logger"/>
            <argument type="service" id="assets.packages"/>
            <argument>%router.request_context.scheme%</argument>
            <argument>%url.host_default%</argument>
        </service>

        <service id="unilend.service.project_request_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\ProjectRequestManager">
            <argument type="service" id="unilend.service.entity_manager"/>
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="unilend.service.project_manager"/>
            <argument type="service" id="unilend.service.wallet_creation_manager"/>
            <argument type="service" id="unilend.frontbundle.service.source_manager"/>
            <argument type="service" id="unilend.service.partner_manager"/>
            <argument type="service" id="unilend.service.eligibility.eligibility_manager"/>
            <argument type="service" id="logger"/>
            <argument type="service" id="unilend.service_product.product_manager"/>
        </service>

        <service id="unilend.service.external_data_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\ExternalDataManager">
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="unilend.service.ws_client.altares_manager"/>
            <argument type="service" id="unilend.service.ws_client.euler_manager"/>
            <argument type="service" id="unilend.service.ws_client.codinf_manager"/>
            <argument type="service" id="unilend.service.ws_client.infolegale_manager"/>
            <argument type="service" id="unilend.service.ws_client.infogreffe_manager"/>
            <argument type="service" id="unilend.service.ws_client.ellisphere_manager"/>
            <argument type="service" id="unilend.service.company_balance_sheet_manager"/>
            <argument type="service" id="unilend.service.risk_data_monitoring_manager"/>
        </service>

        <service id="unilend.service.location_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\LocationManager">
            <argument type="service" id="unilend.service.entity_manager"/>
            <argument>%mapbox.token%</argument>
            <argument type="service" id="memcache.default"/>
        </service>

        <service id="unilend.service.search_service" class="Unilend\Bundle\CoreBusinessBundle\Service\SearchService">
            <argument type="service" id="unilend.service.entity_manager"/>
            <argument type="service" id="translator.default"/>
            <argument type="service" id="router"/>
            <argument>%desk_user%</argument>
            <argument>%desk_password%</argument>
        </service>

        <service id="unilend.service.tax_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\TaxManager">
            <argument type="service" id="unilend.service.entity_manager"/>
            <argument type="service" id="doctrine.orm.entity_manager"/>
        </service>

        <service id="unilend.service.client_status_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\ClientStatusManager">
            <argument type="service" id="unilend.service.entity_manager"/>
            <argument type="service" id="unilend.service.notification_manager"/>
            <argument type="service" id="unilend.service.autobid_settings_manager"/>
            <argument type="service" id="doctrine.orm.entity_manager"/>
        </service>

        <service id="unilend.service.cip_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\CIPManager">
            <argument type="service" id="unilend.service_product.product_manager"/>
            <argument type="service" id="unilend.service_product_contract.contract_manager"/>
            <argument type="service" id="unilend.service.entity_manager"/>
            <argument type="service" id="translator"/>
            <argument type="service" id="doctrine.orm.entity_manager"/>
        </service>

        <service id="unilend.service.ekomi" class="Unilend\Bundle\CoreBusinessBundle\Service\Ekomi">
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="logger"/>
            <argument>%ekomi.url%</argument>
            <argument>%ekomi.shop_id%</argument>
            <argument>%ekomi.password%</argument>
        </service>

        <service id="unilend.service.company_balance_sheet_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\CompanyBalanceSheetManager">
            <argument type="service" id="unilend.service.entity_manager"/>
        </service>

        <service id="unilend.service.project_status_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\ProjectStatusManager">
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="translator"/>
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="number_formatter"/>
            <argument type="service" id="assets.packages"/>
            <argument type="service" id="unilend.swiftmailer.message_provider"/>
            <argument type="service" id="mailer"/>
            <argument type="service" id="logger"/>
            <argument>%router.request_context.scheme%://%url.host_default%</argument>
        </service>

        <service id="unilend.service.partner_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\PartnerManager">
            <argument type="service" id="doctrine.orm.entity_manager"/>
        </service>

        <service id="unilend.service.wallet_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\WalletManager">
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="unilend.service.entity_manager"/>
        </service>

        <service id="unilend.service.operation_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\OperationManager">
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="unilend.service.entity_manager"/>
            <argument type="service" id="unilend.service.wallet_manager"/>
            <argument type="service" id="unilend.service.tax_manager"/>
        </service>

        <service id="unilend.service.wallet_creation_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\WalletCreationManager">
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="logger"/>
        </service>

        <service id="unilend.service.bank_account_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\BankAccountManager">
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="unilend.service.entity_manager"/>
            <argument type="service" id="unilend.service.lender_manager"/>
            <argument type="service" id="logger"/>
            <argument type="service" id="validator"/>
        </service>

        <service id="unilend.service.borrower_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\BorrowerManager">
            <argument type="service" id="unilend.service.entity_manager"/>
        </service>

        <service id="unilend.service.payline_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\PaylineManager">
            <argument type="service" id="unilend.service.entity_manager"/>
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="unilend.swiftmailer.message_provider"/>
            <argument type="service" id="swiftmailer.mailer.default"/>
            <argument type="service" id="unilend.service.client_manager"/>
            <argument type="service" id="unilend.service.operation_manager"/>
            <argument type="service" id="router"/>
            <argument type="service" id="assets.packages"/>
            <argument type="service" id="number_formatter"/>
            <argument>%path.payline%include.php</argument>
            <argument>%kernel.environment%</argument>
            <argument>%payline.merchant_id%</argument>
            <argument>%payline.access_key%</argument>
            <call method="setLogger">
                <argument type="service" id="logger" on-invalid="null"/>
            </call>
        </service>

        <service id="unilend.service.vigilance_rule_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\VigilanceRuleManager">
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="unilend.service.client_vigilance_status_manager"/>
            <argument type="service" id="logger"/>
        </service>

        <service id="unilend.service.client_vigilance_status_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\ClientVigilanceStatusManager">
            <argument type="service" id="doctrine.orm.entity_manager"/>
        </service>

        <service id="unilend.service.slack_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\SlackManager">
            <argument type="service" id="cl_slack.api_client"/>
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="assets.packages"/>
            <argument>%router.request_context.scheme%</argument>
            <argument>%url.host_default%</argument>
            <argument>%url.host_admin%</argument>
            <argument>%slack.default_channel%</argument>
            <argument>%kernel.environment%</argument>
        </service>

        <service id="unilend.service.attachment_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\AttachmentManager">
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="filesystem"/>
            <argument>%path.protected%attachments</argument>
        </service>

        <service id="currency_formatter" class="NumberFormatter">
            <argument>%kernel.default_locale%</argument>
            <argument type="constant">\NumberFormatter::CURRENCY</argument>
        </service>

        <service id="number_formatter" class="NumberFormatter">
            <argument>%kernel.default_locale%</argument>
            <argument type="constant">\NumberFormatter::DEFAULT_STYLE</argument>
        </service>

        <service id="date_formatter" class="IntlDateFormatter">
            <argument>%kernel.default_locale%</argument>
            <argument type="constant">\IntlDateFormatter::MEDIUM</argument>
            <argument type="constant">\IntlDateFormatter::SHORT</argument>
        </service>

        <service id="unilend.service.wire_transfer_out_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\WireTransferOutManager">
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="unilend.service.project_manager"/>
            <argument type="service" id="currency_formatter"/>
            <argument type="service" id="unilend.swiftmailer.message_provider"/>
            <argument type="service" id="mailer"/>
            <argument type="service" id="router"/>
            <argument type="service" id="assets.packages"/>
            <argument type="service" id="unilend.service.operation_manager"/>
            <argument>%router.request_context.scheme%://%url.host_default%</argument>
            <argument>%router.request_context.scheme%://%url.host_admin%</argument>
            <argument type="service" id="logger"/>
        </service>

        <service id="unilend.service.invoice_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\InvoiceManager">
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="knp_snappy.pdf"/>
            <argument type="service" id="twig"/>
            <argument>%path.protected%</argument>
        </service>

        <service id="unilend.service.terms_of_sale_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\TermsOfSaleManager">
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="unilend.service.email_manager"/>
            <argument>%kernel.root_dir%</argument>
            <argument>%kernel.default_locale%</argument>
        </service>

        <service id="unilend.service.lender_operations_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\LenderOperationsManager">
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="translator.default"/>
        </service>

        <service id="unilend.service.ifu_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\IfuManager">
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="memcache.default"/>
        </service>

        <service id="unilend.service.ws_monitoring_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\WsMonitoringManager">
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="unilend.service.ws_client.call_history_handler"/>
        </service>

        <service id="unilend.service.risk_data_monitoring_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\RiskDataMonitoringManager">
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="unilend.service.ws_client.euler_manager"/>
            <argument type="service" id="monolog.logger.wsclient"/>
            <argument type="service" id="translator"/>
        </service>

        <service id="unilend.service.sponsorship_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\SponsorshipManager">
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="unilend.service.operation_manager"/>
            <argument type="service" id="unilend.service.client_status_manager"/>
            <argument type="service" id="number_formatter"/>
            <argument type="service" id="unilend.swiftmailer.message_provider"/>
            <argument type="service" id="mailer"/>
            <argument type="service" id="logger"/>
            <argument type="service" id="router"/>
            <argument type="service" id="assets.packages"/>
            <argument>%router.request_context.scheme%</argument>
            <argument>%url.host_default%</argument>
        </service>

        <service id="unilend.service.lender_validation_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\LenderValidationManager">
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="unilend.service.client_status_manager"/>
            <argument type="service" id="unilend.service.welcome_offer_manager"/>
            <argument type="service" id="unilend.service.sponsorship_manager"/>
            <argument type="service" id="number_formatter"/>
            <argument type="service" id="unilend.swiftmailer.message_provider"/>
            <argument type="service" id="mailer"/>
            <argument type="service" id="logger"/>
            <argument type="service" id="assets.packages"/>
            <argument>%router.request_context.scheme%</argument>
            <argument>%url.host_default%</argument>
        </service>

        <service id="unilend.service.borrower_operations_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\BorrowerOperationsManager">
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="translator.default"/>
            <argument type="service" id="logger"/>
        </service>

        <service id="unilend.service.company_manager" class="Unilend\Bundle\CoreBusinessBundle\Service\CompanyManager">
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="translator"/>
            <argument type="service" id="unilend.service.project_manager"/>
            <argument type="service" id="unilend.service.risk_data_monitoring_manager"/>
        </service>
    </services>
</container>
