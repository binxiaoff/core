<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="unilend.frontbundle.event.source_subscriber" class="Unilend\Bundle\FrontBundle\EventSubscriber\SourceSubscriber">
            <argument type="service" id="unilend.frontbundle.service.source_manager" />
            <tag name="kernel.event_subscriber" />
        </service>
        <service id="unilend.frontbundle.event.data_layer_subscriber" class="Unilend\Bundle\FrontBundle\EventSubscriber\DataLayerSubscriber">
            <argument type="service" id="unilend.frontbundle.service.data_layer_collector" />
            <tag name="kernel.event_subscriber" />
        </service>
        <service id="unilend.frontbundle.event.tos_subscriber" class="Unilend\Bundle\FrontBundle\EventSubscriber\TermsOfServiceSubscriber">
            <argument type="service" id="unilend.service.client_manager" />
            <tag name="kernel.event_subscriber" />
        </service>
        <service id="unilend.frontbundle.event.exception_subscriber" class="Unilend\Bundle\FrontBundle\EventSubscriber\ExceptionSubscriber">
            <argument type="service" id="kernel"/>
            <tag name="kernel.event_subscriber" />
        </service>
    </services>
</container>
