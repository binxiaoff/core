{% block extendHead %}
  <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/9.2.0/styles/monokai-sublime.min.css">
  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/9.2.0/highlight.min.js"></script>
  <script type="text/javascript">if (hljs) hljs.initHighlightingOnLoad()</script>
  <style type="text/css">
    table {
      width: 100%;
    }
    table.table-chart th {
      font-weight: bold;
      text-align: center;
      padding: 5px;
    }
    table.table-chart td {
      border: solid 1px #eee;
      padding: 5px;
    }
    /* First and third cell */
    table.table-chart-list tr td:nth-of-type(0n+1),
    table.table-chart-list tr td:nth-of-type(0n+3) {
      text-align: center;
    }
    table.table-chart-grid td {
      text-align: center;
    }
    .grid-examples .row {
      margin-bottom: 20px;
    }
    .grid-examples .row > * {
      text-align: center;
    }
    .grid-examples .row > :nth-of-type(2n+1) {
      background-color: #eee;
    }
    .grid-examples .row > :nth-of-type(2n+2) {
      background-color: #ddd;
    }
    .resp {
      background-color: #c00;
      color: #fff;
      border-radius: 1em;
      line-height: 1.2em;
      padding: 0 .5em;
    }
    #table-ratings td {
      text-align: center;
    }
    .example {
      display: block;
      padding: 20px;
      background: #fafafa;
      border: solid 1px #ddd;
      border-radius: 4px;
      margin-bottom: 20px;
    }
    code {
      font-family: 'Andale Mono', 'Courier New', mono;
      background-color: #eee;
      border: solid 1px #ccc;
      color: #6789AB;
      padding: 4px 6px;
      font-size: .8em;
      -webkit-font-smoothing: normal;
      -moz-osx-font-smoothing: normal;
    }
    pre {
      font-family: 'Andale Mono', 'Courier New', mono;
      background-color: #333;
      color: #ccc;
      display: block;
      padding: 0;
      margin-top: 20px;
      margin-bottom: 20px;
    }
    pre code {
      background-color: transparent;
      border-color: transparent;
      color: inherit;
      display: block;
      padding: 20px;
      overflow: auto;
      line-height: 1.2em;
    }
    pre code.hljs {
      padding: 20px;
    }
  </style>
{% endblock %}

