<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key=""></parameter>
    </parameters>
    <services>
        <service id="unilend.service.autobid_settings_manager" class="Unilend\Service\AutoBidSettingsManager">
            <argument type="service" id="unilend.service.entity_manager"/>
            <argument type="service" id="unilend.service.client_settings_manager"/>
            <argument type="service" id="unilend.service.client_manager"/>
            <argument type="service" id="unilend.service.notification_manager"/>
            <argument type="service" id="unilend.service.lender_manager"/>
        </service>

        <service id="unilend.service.bid_manager" class="Unilend\Service\BidManager">
            <argument type="service" id="unilend.service.entity_manager"/>
            <argument type="service" id="unilend.service.notification_manager"/>
            <argument type="service" id="unilend.service.autobid_settings_manager"/>
            <argument type="service" id="unilend.service.lender_manager"/>
        </service>

        <service id="unilend.service.client_manager" class="Unilend\Service\ClientManager">
            <argument type="service" id="unilend.service.entity_manager"/>
            <argument type="service" id="unilend.service.client_settings_manager"/>
        </service>

        <service id="unilend.service.lender_manager" class="Unilend\Service\LenderManager">
            <argument type="service" id="unilend.service.entity_manager"/>
        </service>

        <service id="unilend.service.loan_manager" class="Unilend\Service\LoanManager">
            <argument type="service" id="unilend.service.entity_manager"/>
        </service>

        <service id="unilend.service.notification_manager" class="Unilend\Service\NotificationManager">
            <argument type="service" id="unilend.service.entity_manager"/>
            <argument type="service" id="unilend.service.email_manager"/>
        </service>

        <service id="unilend.service.email_manager" class="Unilend\Service\MailerManager">
            <argument type="service" id="unilend.service.entity_manager"/>
        </service>

        <service id="unilend.service.project_manager" class="Unilend\Service\ProjectManager">
            <argument type="service" id="unilend.service.entity_manager"/>
            <argument type="service" id="unilend.service.bid_manager"/>
            <argument type="service" id="unilend.service.loan_manager"/>
            <argument type="service" id="unilend.service.notification_manager"/>
            <argument type="service" id="unilend.service.autobid_settings_manager"/>
            <argument type="service" id="unilend.service.email_manager"/>
            <argument type="service" id="unilend.service.lender_manager"/>
        </service>

        <service id="unilend.service.client_settings_manager" class="Unilend\Service\ClientSettingsManager">
            <argument type="service" id="unilend.service.entity_manager"/>
            <argument type="service" id="memcache.default"/>
        </service>

        <service id="unilend.service.irr_manager" class="Unilend\Service\IRRManager">
            <argument type="service" id="unilend.service.entity_manager"/>
        </service>

        <service id="unilend.service.entity_manager" class="Unilend\Service\EntityManager">
            <argument type="service" id="database_connection"/>
        </service>

        <service id="unilend.service.mail_queue" class="Unilend\Service\MailQueueManager">
            <argument type="service" id="unilend.service.entity_manager"/>
            <argument type="service" id="unilend.swiftmailer.message_provider"/>
        </service>


        <service id="swiftmailer.spool.db" alias="unilend.swiftmailer.db_spool" />
        <service id="swiftmailer.mailer.default.spool.db" alias="unilend.swiftmailer.db_spool" />

        <service id="unilend.swiftmailer.message_provider" class="Unilend\Bridge\SwiftMailer\TemplateMessageProvider">
            <argument type="service" id="unilend.service.entity_manager"/>
            <argument>Unilend\Bridge\SwiftMailer\TemplateMessage</argument>
        </service>

        <service id="unilend.swiftmailer.db_spool" class="Unilend\Bridge\SwiftMailer\DatabaseSpool">
            <argument type="service" id="unilend.service.mail_queue"/>
        </service>

        <service id="unilend.swiftmailer.transport.mailjet.eventdispatcher" class="Unilend\Bridge\SwiftMailer\MailjetTransportEventDispatcher" parent="swiftmailer.transport.eventdispatcher.abstract" />

        <service id="unilend.swiftmailer.transport.mailjet" class="Unilend\Bridge\SwiftMailer\MailjetTransport">
            <argument type="service" id="unilend.swiftmailer.transport.mailjet.eventdispatcher"/>
            <argument type="service" id="mailjet.api.client"/>
        </service>

        <service id="mailjet.api.client" class="Mailjet\Client">
            <argument>%mailjet.api_key%</argument>
            <argument>%mailjet.api_secret%</argument>
        </service>

        <service id="cache" class="Unilend\librairies\Cache"/>
    </services>
</container>